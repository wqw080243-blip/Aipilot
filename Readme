TeamsTab:AddButton({Title="Join Home Subs",Callback=function()if teamRemote then teamRemote:FireServer("Home Subs")end end})

-- MISC TAB
MiscTab:AddToggle("Speed",{Title="Speed",Default=false,Callback=function(v)speedEnabled=v if player.Character and player.Character:FindFirstChild("Humanoid")then player.Character.Humanoid.WalkSpeed=v and 100 or 16 end end})
MiscTab:AddSlider("Speed Boost",{Title="Speed Boost",Min=0,Max=84,Default=0,Rounding=0,Callback=function(v)speedBoost=v if player.Character and player.Character:FindFirstChild("Humanoid")then player.Character.Humanoid.WalkSpeed=(speedEnabled and 100 or 16)+v end end})

