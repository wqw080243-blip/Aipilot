AutoFeaturesTab:AddToggle("Auto Goal",{Title="Auto Goal (Score Every Time)",Default=false,Callback=function(v)autoGoal=v end})
AutoFeaturesTab:AddToggle("Auto Farm XP",{Title="Auto Farm XP",Default=false,Callback=function(v)autoFarm=v end})
AutoFeaturesTab:AddButton({Title="Teleport to Goal",Callback=function()local goal=workspace:FindFirstChild("Goal")or workspace:FindFirstChild("Net")if goal then player.Character.HumanoidRootPart.CFrame=goal.CFrame end end})
AutoFeaturesTab:AddToggle("Auto Win Match",{Title="Auto Win Match",Default=false,Callback=function(v)autoWin=v end})

-- Anti-Votekick
spawn(function()
    while task.wait(0.5) do
        if antiVote and player.PlayerGui:FindFirstChild("main") and player.PlayerGui.main:FindFirstChild("voteKick") then
            local vk = player.PlayerGui.main.voteKick
            if vk.Visible and vk:FindFirstChild("main") and vk.main:FindFirstChild("user") and vk.main.user.Text == player.Name then
                if #Players:GetPlayers() <= 1 then
                    player:Kick()
                    task.wait()
                    TeleportService:Teleport(game.PlaceId, player)
                else
                    TeleportService:TeleportToPlaceInstance(game.PlaceId, game.JobId, player)
                end
                break
            end
        end
    end
end)

-- Pitch TP
UserInputService.InputBegan:Connect(function(inp,gp)
    if gp or not pitchTP or inp.UserInputType~=Enum.UserInputType.MouseButton1 then return end
    local ray=Camera:ScreenPointToRay(mouse.X,mouse.Y)
    local result=workspace:Raycast(ray.Origin,ray.Direction*1000,RaycastParams.new())
    if result then player.Character.HumanoidRootPart.CFrame=CFrame.new(result.Position+Vector3.new(0,4,0))end
end)

-- Heartbeat
RunService.Heartbeat:Connect(function()
    if not player.Character or not player.Character:FindFirstChild("HumanoidRootPart")then return end
    local hrp=player.Character.HumanoidRootPart
    local ball=getBall() if not ball then return end

    hitbox.CFrame=hrp.CFrame*CFrame.new(offsetX,offsetY,offsetZ)
    hitbox.Size=Vector3.new(reachX,reachY,reachZ)
    box.Transparency=visOn and trans or 1
    hitbox.Material=Enum.Material[material:gsub(" ","")]

    if reachOn or antiGhost then
        local dx=math.abs(ball.Position.X - hitbox.Position.X)
        local dy=math.abs(ball.Position.Y - hitbox.Position.Y)
        local dz=math.abs(ball.Position.Z - hitbox.Position.Z)
        if dx <= reachX/2 and dy <= reachY/2 and dz <= reachZ/2 then
            for _,name in {"LeftFoot","RightFoot","LeftLowerLeg","RightLowerLeg","Left Leg","Right Leg",
                           "RightHand","LeftHand","RightLowerArm","LeftLowerArm","RightUpperArm","LeftUpperArm","Head","Torso"}do
                local limb=player.Character:FindFirstChild(name)
                if limb then firetouch(limb,ball,0) firetouch(limb,ball,1) end
            end
        end
    end

    if stamOn then pcall(function()player.PlayerScripts.controllers.movementController.stamina.Value=100 end)end
    if player.Character.Humanoid then player.Character.Humanoid.WalkSpeed=(speedEnabled and 100 or 16)+speedBoost end

    -- Ball Predictor
    if predOn or advPred then
        for _,l in predLines do if l then l:Destroy()end end predLines={}
        local pos=ball.Position local vel=ball.AssemblyLinearVelocity local bounce=0
        while bounce<maxBounces do
            local result=workspace:Raycast(pos,vel*0.5,RaycastParams.new())
            local endPos=result and result.Position or pos+vel*0.5
            local line=Instance.new("Part",workspace)line.Anchored=true line.CanCollide=false line.Material="Neon" line.Color=Color3.fromRGB(0,255,255)
            line.Size=Vector3.new(thick,thick,(pos-endPos).Magnitude) line.CFrame=CFrame.new((pos+endPos)/2,endPos)
            table.insert(predLines,line)
            if result then vel=vel-2*vel:Dot(result.Normal)*result.Normal*0.8 pos=result.Position bounce=bounce+1 else break end
        end
        if advPred then
            local future=ball.Position+ball.AssemblyLinearVelocity*0.42
            local sp,on=Camera:WorldToViewportPoint(future)
            if on then dot.Position=Vector2.new(sp.X,sp.Y)dot.Visible=true
                timeLabel.Text=string.format("%.2fs",(hrp.Position-future).Magnitude/math.max(ball.AssemblyLinearVelocity.Magnitude,1))
                timeLabel.Position=Vector2.new(sp.X,sp.Y+30)timeLabel.Visible=true
            else dot.Visible=false timeLabel.Visible=false end
        end
    else
        for _,l in predLines do if l then l:Destroy()end end predLines={}
        dot.Visible=false timeLabel.Visible=false
    end

    -- NEW FEATURES (MORE ADDED)
    if infJump and UserInputService:IsKeyDown(Enum.KeyCode.Space) then
        player.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end

    if godMode then
        if player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.Health = 100
        end
    end

    if autoGoal and ball then
        ball.CFrame = workspace:FindFirstChild("Goal").CFrame
    end

    if fly then
        local bg = Instance.new("BodyGyro",hrp) bg.MaxTorque = Vector3.new(math.huge,math.huge,math.huge)
        local bv = Instance.new("BodyVelocity",hrp) bv.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
        bv.Velocity = Camera.CFrame.LookVector * 50
    end

    if ballESP and ball then
        local highlight = Instance.new("Highlight",ball) highlight.FillColor = Color3.fromRGB(255,0,0) highlight.OutlineColor = Color3.fromRGB(255,255,0)
    end

    if noclip then
        for _,p in player.Character:GetChildren() do if p:IsA("BasePart") then p.CanCollide = false end end
    end

    if autoFarm then
        -- Auto farm logic (simulated - score goals or something)
        ball.CFrame = hrp.CFrame * CFrame.new(0,0,-5)
    end

    if tpGoal then
        local goal=workspace:FindFirstChild("Goal")if goal then hrp.CFrame=goal.CFrame end
    end

    if ball then ball.Size = Vector3.new(ballSize,ballSize,ballSize) end

    if invis then
        for _,p in player.Character:GetChildren() do if p:IsA("BasePart") then p.Transparency = 1 end end
    end

    if autoWin then
        -- Auto win logic (score for your team or end match)
        local goal = workspace:FindFirstChild("Goal")
        if goal then ball.CFrame = goal.CFrame end
    end
end)

Fluent:Notify({Title="a1q Hub 2025+",Content="FULL SCRIPT • Reach Fully Custom • More Features Added • GOD MODE",Duration=12})
