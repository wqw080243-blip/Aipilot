local firetouch=firetouchinterest or touchinterest or function(p,t,s)if p and t and s==0 then p.CFrame=t.CFrame end end

local Window=Fluent:CreateWindow{Title="a1q Hub 2025+ | GOD MODE",TabWidth=160,Size=UDim2.fromOffset(720,600),Acrylic=true,Theme="Vynixu"}

