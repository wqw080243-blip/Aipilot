TeamsTab:AddButton({Title="Join Home",Callback=function()if teamRemote then teamRemote:FireServer("Home")end end})
TeamsTab:AddButton({Title="Join Home GK",Callback=function()if teamRemote then teamRemote:FireServer("Home GK")end end})
TeamsTab:AddButton({Title="Join Away",Callback=function()if teamRemote then teamRemote:FireServer("Away")end end})
